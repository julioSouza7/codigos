#include <stdio.h>

int main() {
    int vetor[5], soma = 0;

    // Preenchendo o vetor e calculando a soma
    for (int i = 0; i < 5; i++) {
        printf("Digite o valor para vetor[%d]: ", i);
        scanf("%d", &vetor[i]);
        soma += vetor[i];
    }

    // Exibindo a soma
    printf("A soma dos elementos do vetor Ã©: %d\n", soma);

    return 0;
}

#include <stdio.h>

int main() {
    int a, b;
    printf("Digite o primeiro número: ");
    scanf("%d", &a);
    printf("Digite o segundo número: ");
    scanf("%d", &b);

    if (a > b) {
        printf("%d é maior que %d.\n", a, b);
    } else if (a < b) {
        printf("%d é menor que %d.\n", a, b);
    } else {
        printf("%d é igual a %d.\n", a, b);
    }

    return 0;
}

#include <stdio.h>

#define MAX 100

int fibonacci(int n, int memo[]) {
    if (n <= 1) {
        return n;
    }

    if (memo[n] == -1) {
        memo[n] = fibonacci(n - 1, memo) + fibonacci(n - 2, memo);
    }

    return memo[n];
}

int main() {
    int n;
    printf("Digite o número de termos da sequência de Fibonacci: ");
    scanf("%d", &n);

    int memo[MAX];
    for (int i = 0; i < MAX; i++) {
        memo[i] = -1;
    }

    int i = 0;
    while (i < n) {
        printf("%d ", fibonacci(i, memo));
        i++;
    }
    printf("\n");

    return 0;
}

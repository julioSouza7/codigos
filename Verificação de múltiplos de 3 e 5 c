#include <stdio.h>

int main() {
    int num;
    printf("Digite um número: ");
    scanf("%d", &num);

    if (num % 3 == 0 && num % 5 == 0) {
        printf("%d é múltiplo de 3 e 5.\n", num);
    } else if (num % 3 == 0) {
        printf("%d é múltiplo de 3.\n", num);
    } else if (num % 5 == 0) {
        printf("%d é múltiplo de 5.\n", num);
    } else {
        printf("%d não é múltiplo de 3 nem de 5.\n", num);
    }

    return 0;
}

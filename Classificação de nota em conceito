#include <stdio.h>

int main() {
    float nota;

    printf("Digite a nota do aluno: ");
    scanf("%f", &nota);

    if (nota >= 9.0 && nota <= 10.0) {
        printf("Conceito: A\n");
    } else if (nota >= 7.0 && nota < 9.0) {
        printf("Conceito: B\n");
    } else if (nota >= 5.0 && nota < 7.0) {
        printf("Conceito: C\n");
    } else if (nota >= 3.0 && nota < 5.0) {
        printf("Conceito: D\n");
    } else if (nota >= 0.0 && nota < 3.0) {
        printf("Conceito: E\n");
    } else {
        printf("Nota invÃ¡lida.\n");
    }

    return 0;
}

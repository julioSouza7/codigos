#include <stdio.h>

int main() {
    int vetor[5];

    // Preenchendo o vetor
    for (int i = 0; i < 5; i++) {
        printf("Digite o valor para vetor[%d]: ", i);
        scanf("%d", &vetor[i]);
    }

    // Exibindo o vetor
    printf("Elementos do vetor: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}
